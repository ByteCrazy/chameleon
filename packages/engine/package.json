{
  "name": "@chamn/engine",
  "version": "0.0.2",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run build:render && build-script",
    "build": "build-script --build",
    "build:app": "npm run build:render && export NODE_OPTIONS=--max-old-space-size=32768 && cross-env BUILD_TYPE=APP build-script --build",
    "build:render": "cross-env BUILD_TYPE=Render build-script --build",
    "build:app:analyze": "cross-env BUILD_TYPE=APP ANALYZE=1 build-script --build",
    "lint": "eslint --ext .tsx,.ts src/",
    "prettier": "prettier --write ./src",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@chamn/layout": "workspace:*",
    "@chamn/model": "workspace:*",
    "@chamn/render": "workspace:*",
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@monaco-editor/react": "^4.4.6",
    "ahooks": "^3.7.4",
    "antd": "^5.0.0",
    "consola": "^2.15.3",
    "i18next": "^22.1.5",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "monaco-editor": "^0.34.1",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@chamn/build-script": "workspace:*",
    "@chamn/demo-page": "workspace:*",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/consola": "^2.2.5",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rc-select": "^14.4.0",
    "react-router-dom": "^6.7.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.54.0",
    "vite-plugin-monaco-editor": "^1.1.0"
  },
  "config": {}
